    "@nuxt/devtools": "^1.0.0",
    "@nuxt/eslint-config": "^0.2.0",
    "@types/node": "^20.8.2",
    "eslint": "^8.50.0",
    "nuxt": "^3.7.4",
    "wrangler": "^3.16.0"
  }
}
